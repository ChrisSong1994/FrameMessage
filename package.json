  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-strip": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "fs-extra": "^9.0.1",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.0.3"
  }
